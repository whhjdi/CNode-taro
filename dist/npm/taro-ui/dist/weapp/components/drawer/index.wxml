<block>
    <block wx:if="{{_show}}">
        <view class="{{anonymousState__temp}}">
            <view class="at-drawer__mask" style="{{anonymousState__temp2}}" bindtap="onMaskClick" data-e-tap-so="this"></view>
            <view class="at-drawer__content" style="{{anonymousState__temp3}}">
                <block>
                    <block wx:if="{{items.length}}">
                        <at-list __triggerObserer="{{ _triggerObserer }}">
                            <at-list-item wx:key="index" __triggerObserer="{{ _triggerObserer }}" data-index="{{index}}" bindonclick="onItemClick" title="{{name}}" arrow="right" wx:for="{{items}}" wx:for-item="name" wx:for-index="index" __fn_onClick="{{true}}" data-e-onclick-so="this" data-e-onclick-a-a="{{index}}"></at-list-item>
                        </at-list>
                    </block>
                    <block wx:else>
                        <slot></slot>
                    </block>
                </block>
            </view>
        </view>
    </block>
</block>